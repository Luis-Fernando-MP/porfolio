{
  "name": "porfolio",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "dev": "rimraf .next && pnpm store prune && next dev",
    "build": "pnpm lint && next build",
    "start": "next start",
    "lint": "pnpx stylelint '**/*.{css,scss}' && next lint",
    "format": "pnpx stylelint '**/*.{css,scss}' --fix && prettier --write '**/*.{js,jsx,ts,tsx,json,md,scss,css}'",
    "notion:generate": "ts-node --project ./notion.tsconfig.json -r tsconfig-paths/register ./notion/index.ts",
    "postinstall": "prisma generate",
    "prisma:migrate": "pnpx prisma migrate dev --name",
    "prisma:deploy": "pnpx prisma migrate deploy",
    "prisma:generate": "pnpx prisma generate",
    "build:content": "contentlayer2 build"
  },
